<!DOCTYPE html>
<html>
<head>
    <title>User List - <%= shopData.shopName %></title>
    <link rel="stylesheet" type="text/css" href="/main.css">
</head>
<body>
    <h1>User List for <%= shopData.shopName %></h1>

    <table>
        <thead>
            <tr>
                <th>Username</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td><%= user.username %></td>
                    <td><%= user.first_name %></td>
                    <td><%= user.last_name %></td>
                    <td><%= user.email %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
